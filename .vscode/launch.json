	"version": "0.2.0",
	"configurations": [
		{
			"name": "debug-current",
			"program": "${file}",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "node"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "debug-index",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.js",
			"console": "integratedTerminal"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "debug-index-2",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.js",
			"console": "internalConsole"
		}
	]
}
